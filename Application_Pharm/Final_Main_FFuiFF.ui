                                 <string notr="true">/* Styling for QScrollArea */
QScrollArea {
    background-color: #f0f0f0; /* Background color */
    border: 1px solid rgb(17, 180, 180); /* Border color */
    border-radius: 10px; /* Border radius */
    padding: 2px;
}

/* Styling for QScrollBar */
QScrollBar:vertical {
    border: none;
    background: transparent;
    width: 10px; /* Width of the scrollbar */
    margin: 0px 0px 0px 0px;
}

QScrollBar::handle:vertical {
    background: rgb(17, 180, 180); /* Color of the scrollbar handle */
    border-radius: 5px; /* Handle border radius */
}

QScrollBar::handle:vertical:hover {
    background: #bbb; /* Color of the scrollbar handle on hover */
}

QScrollBar::add-line:vertical {
    border: none;
    background: transparent;
    height: 0px;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:vertical {
    border: 2px;
    background: transparent;
    height: 0px;
    subcontrol-position: top;
    subcontrol-origin: margin;
}

/* Styling for QScrollBar arrows */
QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical {
    border: none;
    background: transparent;
    width: 0px;
    height: 0px;
}

/* Styling for QScrollBar page up and page down */
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    background: transparent;
}

/* Styling for QFrame inside QScrollArea (cards) */
QScrollArea QFrame {
    background-color: #ffffff; /* Card background color */
    border: 0px solid rgb(17, 180, 180); /* Card border color */
    border-radius: 10px; /* Card border radius */
}

/* Styling for QCheckBox inside the Card */
QScrollArea QFrame QCheckBox {
    color: #000000; /* Checkbox text color */
}

QScrollArea QFrame QPushButton {
    background-color:transparent ; /* Button background color */
    color: #000000; /* Button text color */
    border: 0.5px solid black ; /* No border */
    border-radius: 5px; /* Button border radius */
}

/* Hover effect for QPushButton */
QScrollArea QFrame QPushButton:hover {
    background-color: rgb(255, 255, 255); /* Button background color on hover */
    color: black; /* Button text color on hover */
}

/* Styling for QLabel inside the Card */
QScrollArea QFrame QLabel {
    color: #000000; /* Label text color */
    font-size: 12px; /* Font size */
}



